<ResourceDictionary 
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:local="using:XElement.RedYellowBlue.UI.UWP.Modules.FlaticonCredit" 

                    xmlns:svg="using:Mntone.SvgForXaml.UI.Xaml">

    <Style x:Key="ImageContainerStyle" TargetType="Border">
        <Setter Property="Background" Value="White" />
        <Setter Property="Margin" Value="0 0 4 0" />
        <Setter Property="Padding" Value="1" />
    </Style>

    <Style x:Key="ImageStyle" TargetType="svg:SvgImage">
        <Setter Property="Height" Value="16" />
        <Setter Property="Width" Value="16" />
    </Style>

</ResourceDictionary>
